{
    "extends": ["airbnb-base", "plugin:jest/recommended"],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": [2, "never"],
        "quotes": [2, "single"],
        "linebreak-style": 0,
        "indent": [2, 2, {"SwitchCase": 1}],
        "consistent-return": 1
    },
    "plugins": [
        "jest"
    ]
}